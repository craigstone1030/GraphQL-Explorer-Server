{
  "name": "GraphQL-Explorer",
  "version": "1.0.0",
  "description": "Full API & Ability Usage of GraphQL.",
  "scripts": {
    "dev": "npm-run-all --parallel dev:server dev:client",
    "build": "npm-run-all --parallel build:server build:client",
    "clean": "node ./scripts/clean.js --server --client",
    "dev:client": "cross-env NODE_ENV=development parcel ./client/public/index.html --out-dir ./client/dist --port 5999",
    "test:client": "cross-env NODE_ENV=test jest --forceExit",
    "build:client": "cross-env NODE_ENV=production parcel build ./client/public/index.html --out-dir ./client-dist",
    "dev:server": "cross-env NODE_ENV=development nodemon ./server/app.ts",
    "build:server": "node ./scripts/clean.js --server && tsc",
    "start:server": "cross-env NODE_ENV=production npm run build:server && nodemon ./server-dist/app.js",
    "pm2": "npm run build:server && pm2 start ecosystem.config.js",
    "stress-test": "node ./scripts/st-main.js",
    "voyager": "serve -s voyager",
    "gen:code": "graphql-codegen --config codegen.yml",
    "gen:docs": "rimraf -rf ./docs && graphdoc --force",
    "serve:docs": "npm run gen:docs && serve -s docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linbudu599/GraphQL-Explorer.git"
  },
  "keywords": [
    "GraphQL",
    "Parcel",
    "TypeGraphQL",
    "Apollo-Server",
    "Apollo-Client"
  ],
  "graphdoc": {
    "endpoint": "http://localhost:4000/graphql",
    "output": "./docs"
  },
  "author": "linbudu599",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/linbudu599/GraphQL-Explorer/issues"
  },
  "homepage": "https://github.com/linbudu599/GraphQL-Explorer#readme",
  "dependencies": {
    "@2fd/graphdoc": "^2.4.0",
    "@apollo/react-hooks": "^4.0.0",
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "apollo-server": "^2.17.0",
    "apollo-server-koa": "^2.17.0",
    "apollo-server-plugin-base": "^0.10.2",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cross-env": "^7.0.2",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "graphql-voyager": "^1.0.0-rc.31",
    "graphqldoc": "^2.5.2",
    "koa": "^2.13.0",
    "minimist": "^1.2.5",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "pm2": "^4.5.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "serve": "^11.3.2",
    "sqlite3": "^5.0.0",
    "type-graphql": "^1.0.0",
    "type-graphql-dataloader": "^0.3.7",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.25",
    "typeorm-typedi-extensions": "^0.2.3",
    "typescript": "^4.0.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "@graphql-codegen/cli": "^1.19.2",
    "@graphql-codegen/fragment-matcher": "^2.0.0",
    "@graphql-codegen/introspection": "^1.18.0",
    "@graphql-codegen/typescript": "^1.17.11",
    "@graphql-codegen/typescript-operations": "^1.17.10",
    "@graphql-codegen/typescript-react-apollo": "^2.1.1",
    "@graphql-codegen/typescript-resolvers": "^1.17.11",
    "@ice/spec": "^1.0.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.1",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.13",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^14.6.3",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "apollo": "^2.30.3",
    "apollo-server-testing": "^2.17.0",
    "babel-jest": "^26.3.0",
    "babel-plugin-transform-async-to-promises": "^0.8.15",
    "chalk": "^4.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "jest": "^26.4.2",
    "jest-enzyme": "^7.1.2",
    "nodemon": "^2.0.4",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "superagent": "^6.1.0",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0"
  }
}
