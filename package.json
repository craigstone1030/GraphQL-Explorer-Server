{
  "name": "GraphQL-Explorer",
  "version": "1.0.0",
  "description": "Full API & Ability Usage of Apollo Client/Server.",
  "scripts": {
    "dev": "npm-run-all --parallel dev:server dev:client",
    "dev:client": "cross-env NODE_ENV=development parcel ./client/public/index.html --out-dir ./client/dist --target browser --cache-dir ./client/.cache --port 5999 --log-level 3",
    "test:client": "cross-env TEST=development jest --forceExit",
    "build:client": "cross-env NODE_ENV=production parcel build ./client/public/index.html --out-dir ./client/dist --target bwoser --cache-dir ./client/.cache --experimental-scope-hoisting",
    "clean:server": "rimraf ./server/\\*\\*/\\*.js ./server/\\*\\*/\\*.d.ts",
    "dev:server": "cross-env NODE_ENV=development nodemon ./server/app.ts",
    "compile:server": "npm run clean:server && tsc",
    "start:server": "cross-env NODE_ENV=production npm run compile:server && nodemon ./server/app.js",
    "pm2:dev": "npm run compile:server && pm2 start ecosystem.config.js",
    "pressure-test": "node ./pt-main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linbudu599/GraphQL-Explorer.git"
  },
  "keywords": [
    "GraphQL",
    "Parcel",
    "TypeGraphQL",
    "Apollo-Server",
    "Apollo-Client"
  ],
  "author": "linbudu599",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/linbudu599/GraphQL-Explorer/issues"
  },
  "homepage": "https://github.com/linbudu599/GraphQL-Explorer#readme",
  "dependencies": {
    "@apollo/react-hooks": "^4.0.0",
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-datasource-rest": "^0.9.3",
    "apollo-link-http": "^1.5.17",
    "apollo-server": "^2.17.0",
    "apollo-server-koa": "^2.17.0",
    "apollo-server-plugin-base": "^0.10.2",
    "apollo-upload-client": "^14.1.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "graphql-upload": "^11.0.0",
    "koa": "^2.13.0",
    "log4js": "^6.3.0",
    "mkdirp": "^1.0.4",
    "npm-run": "^5.0.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "pm2": "^4.5.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sqlite3": "^5.0.0",
    "type-graphql": "^1.0.0",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.25",
    "typeorm-typedi-extensions": "^0.2.3",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "@ice/spec": "^1.0.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.1",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.13",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^14.6.3",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "apollo": "^2.30.3",
    "apollo-server-testing": "^2.17.0",
    "babel-jest": "^26.3.0",
    "babel-plugin-transform-async-to-promises": "^0.8.15",
    "chalk": "^4.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "jest": "^26.4.2",
    "jest-enzyme": "^7.1.2",
    "nock": "^13.0.4",
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.4",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "superagent": "^6.1.0",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0"
  }
}
